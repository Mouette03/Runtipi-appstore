{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "triliumnext",
      "image": "ghcr.io/triliumnext/trilium:v0.98.1",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "TRILIUM_DATA_DIR",
          "value": "/home/node/trilium-data"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/home/node/trilium-data"
        }
      ]
    }
  ]
}
