{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "duckdns",
      "image": "lscr.io/linuxserver/duckdns:992f1854-ls62",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config",
          "readOnly": false
        }
      ],
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "SUBDOMAINS",
          "value": "${DUCKDNS_SUBDOMAINS}"
        },
        {
          "key": "TOKEN",
          "value": "${DUCKDNS_TOKEN}"
        },
        {
          "key": "UPDATE_IP",
          "value": "${DUCKDNS_UPDATE_IP}"
        },
        {
          "key": "LOG_FILE",
          "value": "${DUCKDNS_ENABLE_LOG_FILE}"
        }
      ]
    }
  ]
}

